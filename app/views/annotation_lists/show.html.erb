<div class="container">
    <div class="page-header">
        <h1>List: <%= @annotation_list.id %></h1>
    </div>
    <div class="col-sm-12 col-md-12">

        <p id="notice"><%= notice %></p>

        <p>
            <b>Identifier (URI):</b>
            <%= @annotation_list.list_id %>
        </p>

        <p>
            <b>Label:</b>
            <%= @annotation_list.label %>
        </p>
        <p>
            <b>Type:</b>
            <%= @annotation_list.list_type %>
        </p>

        <p>
            <b>Within:</b>
            <%= LayerListsMap.getLayersForList @annotation_list.list_id %>
        </p>

        <p>
            <b>Resources:</b>
            <%= ListAnnotationsMap.getAnnotationsForList  @annotation_list.list_id     %>
         </p>

        <%#= link_to 'Edit', edit_annotation_list_path(@annotation_list) %> |
        <%#= link_to 'Back', annotation_list_path %>
    </div>
</div>


